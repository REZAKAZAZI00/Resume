@model List<ProjectDetailsViewModel>
@foreach (var item in Model)
{
    <div class="col-lg-4 col-md-4 col-sm-6 @item.CategoryTitle">
        <div class="h_gallery_item">
            <div class="g_img_item">
                <img class="img-fluid" src="img/Project/@item.PictureName" alt="@item.Title">
                <a class="light" href="img/Project/@item.PictureName"><img src="img/gallery/icon.png" alt=""></a>
            </div>
            <div class="g_item_text">
                <h4>
                    @item.Title
                </h4>
                <p>@item.CategoryTitle</p>
            </div>
        </div>
    </div>
}

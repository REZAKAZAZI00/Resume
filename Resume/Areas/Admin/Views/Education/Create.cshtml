@model CreateEducationViewModel
@{
    ViewData["Title"] = "Create Skill";
    var result = ViewData["result"] as OutPutModel<bool>;
}
<div class="container">
    <div class="page-inner">
        <div class="page-header">
            <h3 class="fw-bold mb-3"></h3>
            <ul class="breadcrumbs mb-3">
                <li class="nav-home">
                    <a href="#">
                        <i class="icon-home"></i>
                    </a>
                </li>
                <li class="separator">
                    Dashboard
                    <i class="icon-arrow-right"></i>
                </li>
                <li class="nav-item">
                    <a href="#">Educations</a>
                </li>
                <li class="separator">
                    <i class="icon-arrow-right"></i>
                </li>
                <li class="nav-item">
                    <a href="#">Create Education</a>
                </li>
            </ul>
        </div>
        <div class="row">
            <div class="col-md-12">
                @if (result != null)
                {
                    if (result.StatusCode.Equals(200))
                    {
                        <div class="alert @(result.StatusCode.Equals(200)? "alert-success":"alert-warning" ) ">
                            <p class="help-block">@result.Message</p>
                        </div>

                    }
                   
                }
                <div class="card">
                    <div class="card-header">
                        <div class="card-title">Create Skill </div>
                    </div>


                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-8 col-lg-6">
                                <form asp-action="create" method="post">

                                    <div class="form-group">
                                        <label for="email2">InstitutionName</label>
                                        <input type="text"
                                               class="form-control"
                                               id="email2"
                                               asp-for="InstitutionName" />

                                    </div>
                                    <div class="form-group">
                                        <label for="email2">Degree </label>
                                        <input type="text"
                                               class="form-control"
                                               id="email2"
                                               asp-for="Degree" />
                                      
                                    </div>
                                    <div class="form-group">
                                        <label for="email2">FieldOfStudy </label>
                                        <input type="text"
                                               class="form-control"
                                               id="email2"
                                               asp-for="FieldOfStudy" />

                                    </div>
                                    <div class="form-group">
                                        <label for="email2">Description </label>
                                        <input type="text"
                                               class="form-control"
                                               id="email2"
                                               asp-for="Description" />

                                    </div>


                                    <div class="form-group">
                                        <label for="email2">StartDate</label>
                                        <input type="date"
                                               class="form-control"
                                               id="email2"
                                               asp-for="StartDate" />

                                    </div>
                                    <div class="form-group">
                                        <label for="email2">EndDate</label>
                                        <input type="date"
                                               class="form-control"
                                               id="email2"
                                               asp-for="EndDate" />

                                    </div>
                                    <div class="card-action">
                                        <button class="btn btn-success">Submit</button>

                                    </div>

                                </form>


                            </div>

                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
<script>
    document.getElementById('endDate').value = '';
</script>